pipeline {
  agent any
  stages {
    stage ("checkout") {
      steps {
        echo "checked out from git"
	}
    }
    stage ("build") {
      steps {
        echo "building the package"
	}
    }
    stage ("testing") {
      steps {
        echo "tested"
	}
    }
    stage ("from master") {
      steps {
        echo "from the master branch"
	}
    }
  }
}
